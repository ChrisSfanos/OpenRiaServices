<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <NoWarn>618</NoWarn>
    <RootNamespace>DbContextModels</RootNamespace>
    <AssemblyName>EFCoreDbContextModels</AssemblyName>
    <TargetFramework>net472</TargetFramework>
    <Version>1.0.0.0</Version>
  </PropertyGroup>
  <Target Name="WriteProjectPath" BeforeTargets="GetCopyToOutputDirectoryItems">
    <Message Text="Writing project path" />
    <WriteLinesToFile File="ProjectPath.txt" Lines="$(MSBuildProjectFullPath),Generated_Code," Overwrite="true" />
  </Target>
  <ItemGroup>
    <Reference Include="System.ComponentModel.Composition" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="System.Data.Linq" />
    <Reference Include="System.Security" />
    <Reference Include="System.Web" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Northwind\Category.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\Customer.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\CustomerDemographic.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\Employee.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\NorthwindDbCtx.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>NorthwindDbCtx.edmx</DependentUpon>
    </Compile>
    <Compile Update="Northwind\NorthwindEntities.Context.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>NorthwindEntities.Context.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\NorthwindEntities.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\Order.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\Order_Detail.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\Product.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\Region.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\Shipper.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\Supplier.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
    <Compile Update="Northwind\Territory.cs">
      <DependentUpon>NorthwindEntities.tt</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EntityDeploy Include="Northwind\NorthwindDbCtx.edmx">
      <Generator>EntityModelCodeGenerator</Generator>
      <LastGenOutput>NorthwindDbCtx.Designer.cs</LastGenOutput>
      <CustomToolNamespace>DbContextModels</CustomToolNamespace>
    </EntityDeploy>
  </ItemGroup>
  <ItemGroup>
    <None Include="Northwind\NorthwindEntities.Context.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>NorthwindEntities.Context.cs</LastGenOutput>
    </None>
    <None Include="Northwind\NorthwindEntities.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>NorthwindEntities.cs</LastGenOutput>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.20" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\OpenRiaServices.Server\Framework\OpenRiaServices.Server.csproj" />
  </ItemGroup>
</Project>